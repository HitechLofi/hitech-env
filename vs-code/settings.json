{
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "d"
      ],
      "after": [
        "\"",
        "_",
        "d"
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<C-u>"
      ],
      "after": [
        "2",
        "0",
        "k",
        "z",
        "t"
      ]
    },
    {
      "before": [
        "<C-d>"
      ],
      "after": [
        "2",
        "0",
        "j",
        "z",
        "b"
      ]
    },
    {
      "before": [
        "<space>",
        "<space>"
      ],
      "commands": [
        "workbench.action.showCommands"
      ]
    },
    {
      "before": [
        "<space>",
        "d"
      ],
      "commands": [
        "editor.action.revealDefinition"
      ]
    },
    {
      "before": [
        "<space>",
        ","
      ],
      "commands": [
        "workbench.action.openGlobalKeybindings"
      ]
    },
    {
      "before": [
        "<space>",
        "o"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<space>",
        "f"
      ],
      "commands": [
        "workbench.action.findInFiles"
      ]
    },
    {
      "before": [
        "<space>",
        "t"
      ],
      "commands": [
        "editor.action.goToTypeDefinition"
      ]
    },
    {
      "before": [
        "<space>",
        "i"
      ],
      "commands": [
        "editor.action.showHover"
      ]
    },
    {
      "before": [
        "<space>",
        "s"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "<space>",
        "z"
      ],
      "commands": [
        "workbench.action.toggleZenMode"
      ]
    },
    {
      "before": [
        "<space>",
        "b"
      ],
      "commands": [
        "workbench.action.navigateBack"
      ]
    },
    {
      "before": [
        "<space>",
        "r"
      ],
      "commands": [
        "editor.action.referenceSearch.trigger"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "u"
      ],
      "commands": [
        "git.revertChange"
      ]
    },
    {
      "before": [
        "g",
        "p"
      ],
      "commands": [
        "workbench.action.editor.previousChange"
      ]
    },
    {
      "before": [
        "g",
        "n"
      ],
      "commands": [
        "workbench.action.editor.nextChange"
      ]
    },
    {
      "before": [
        "u"
      ],
      "commands": [
        "undo"
      ]
    },
    {
      "before": [
        "<C-r>"
      ],
      "commands": [
        "redo"
      ]
    },
    {
      "before": [
        "<space>",
        "q"
      ],
      "commands": [
        ":q"
      ]
    },
    {
      "before": [
        "<space>",
        "p"
      ],
      "commands": [
        "workbench.action.openSettingsJson"
      ]
    },
    {
      "before": [
        "="
      ],
      "commands": [
        "editor.action.reindentlines"
      ]
    },
    {
      "before": [
        "<space>",
        "k"
      ],
      "commands": [
        ":noh"
      ]
    },
    {
      "before": [
        "<space>",
        "n"
      ],
      "commands": [
        ":tabnext"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        "="
      ],
      "commands": [
        "editor.action.reindentselectedlines"
      ]
    }
  ],
  "vim.autoindent": true,
  "zenMode.hideTabs": true,
  "zenMode.fullScreen": false,
  "editor.lineHeight": 22,
  "editor.lineNumbers": "relative",
  "git.autofetch": true,
  "editor.tabSize": 2,
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.tabCompletion": "on",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorWidth": 3,
  "editor.formatOnPaste": true,
  "editor.fontFamily": "Fira Code Retina",
  "editor.fontLigatures": true,
  "workbench.tree.indent": 10,
  "workbench.activityBar.visible": true,
  "explorer.openEditors.visible": 0,
  "window.menuBarVisibility": "default",
  "breadcrumbs.enabled": true,
  "editor.cursorSurroundingLines": 7,
  "editor.fontSize": 13,
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "debug.inlineValues": true,
  "vim.camelCaseMotion.enable": true,
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "underline",
  "vim.cursorStylePerMode.replace": "underline",
  "vim.cursorStylePerMode.visual": "block",
  "vim.cursorStylePerMode.visualblock": "block",
  "vim.cursorStylePerMode.visualline": "block",
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.visualstar": true,
  "javascript.format.semicolons": "insert",
  "javascript.preferences.quoteStyle": "single",
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.referencesCodeLens.enabled": true,
  "typescript.preferences.quoteStyle": "single",
  "typescript.suggest.completeFunctionCalls": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true
  },
  "workbench.colorTheme": "BeardedTheme Solarized",
  "editor.formatOnSave": true,
  "editor.minimap.scale": 3,
  "editor.minimap.side": "left",
  "editor.codeLens": false,
  "editor.columnSelection": false,
  "editor.fastScrollSensitivity": 10,
  "editor.mouseWheelScrollSensitivity": 2,
  "editor.renderIndentGuides": false,
  "editor.suggest.statusBar.visible": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.tree.renderIndentGuides": "none",
  "workbench.view.alwaysShowHeaderActions": true,
  "search.smartCase": true,
  "problems.showCurrentInStatus": true,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "better-comments.highlightPlainText": true,
  "editor.minimap.maxColumn": 50,
  "editor.minimap.enabled": true,
  "editor.matchBrackets": "never",
  "bracket-pair-colorizer-2.showHorizontalScopeLine": false,
  "bracket-pair-colorizer-2.showVerticalScopeLine": false,
  "bracket-pair-colorizer-2.highlightActiveScope": true,
  "editor.renderLineHighlight": "none",
  "workbench.editor.highlightModifiedTabs": true
}