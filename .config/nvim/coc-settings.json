{
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 20,
  "coc.preferences.formatOnSaveFiletypes": ["ts", "tsx", "elm", "typescript", "typescriptreact"],
  "prettier.singleQuote": true,
  "suggest.autoTrigger": "trigger",
  "suggest.snippetIndicator": "â†’",
  "suggest.enablePreview": true,
  "suggest.timeout": 5000,
  "suggest.noselect": true,
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "~/Documents/Snippets"
  ],
  "rust-analyzer.server.path": "/usr/local/bin/rust-analyzer",
  "diagnostic.enableMessage": "never",
  "diagnostic.errorSign": "ðŸ’©",
  "diagnostic.warningSign": "ðŸ¤”",
  "diagnostic.infoSign": "ðŸ¤”",
  "diagnostic.maxWindowHeight": 30,
  "diagnostic.maxWindowWidth": 110,
  "diagnostic.messageTarget": "echo",
  "codeLens.enable": false,
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "list.insertMappings": {
    "<C-c>": "feedkeys:\\<Esc>",
    "<C-r>": "do:refresh",
    "<C-n>": "normal:j",
    "<C-p>": "normal:k"
  },
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmAnalyseTrigger": "change"
      }
    }
  }
}
